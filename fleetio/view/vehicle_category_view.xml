<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="vehicle_category_tree_view" model="ir.ui.view">
        <field name="name">Vehicle Category Tree View</field>
        <field name="model">vehicle.category</field>
        <field name="arch" type="xml">
            <tree string="Model Category" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="vechile_category_action" model="ir.actions.act_window">
        <field name="name">Category</field>
        <field name="res_model">vehicle.category</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create a vehicle category
            </p>
        </field>
    </record>
</odoo>